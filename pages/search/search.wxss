.hSearch {
  width: 750rpx;
  height: 112rpx;
  padding: 20rpx;
  background: #f2f2f2;
  position: relative;
  box-sizing: border-box;
  padding-left: 140rpx;
}
.hSearch .back {
  font-size: 24rpx;
  color: #333;
  line-height: 112rpx;
  position: absolute;
  top:50%;
  left: 30rpx;
  margin-top: -56rpx;
  padding-left: 30rpx;
}
.hSearch .back .img {
  width: 14rpx;
  height: 24rpx;
  position: absolute;
  top:50%;
  left: 0;
  margin-top: -12rpx;
}
.hSearch .search {
  width: 100%;
  padding: 3px 15px;
  height: 72rpx;
  line-height: 72rpx;
  background: #fff;
  border-radius: 40rpx;
  float: left;
  box-sizing: border-box;
  position: relative;
  border:1rpx solid #FF831B;
}
.hSearch .search .img {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 50%;
  left: 25rpx;
  margin-top: -15rpx;
}
.hSearch .search .ipt {
  width: 100%;
  height: 72rpx;
  line-height: 72rpx;
  font-size: 24rpx;
  position: absolute;
  top: 0;
  left: 70rpx;
  background: transparent;
  border: none;
}

.pageSearch .main {
  text-align: center;
  height: 370rpx;
  position: absolute;
  top:50%;
  left: 0;
  right: 0;
  margin-top: -185rpx;
}
.pageSearch .main .img {
  width: 202rpx;
  height: 204rpx;
  margin-bottom: 65rpx;
}
.pageSearch .main .txt {
  font-size: 28rpx;
  color: #FF831B;
  margin-bottom: 23rpx;
}